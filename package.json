{
  "name": "svelte-app",
  "version": "1.0.0",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public",
    "prettify": "npx prettier --write --plugin-search-dir=. --svelte-sort-order scripts-markup-styles \"src/**\" \"tests/**\"",
    "lint": "npx prettier --check --svelte-sort-order scripts-markup-styles \"src/**\" \"tests/**\" && npx eslint src/components/*.svelte",
    "test": "jest tests"
